{
	"name": "Github Lines of Code",
	"description": "Count lines of code in a Github project",
	"version": "1.2.2",
	"manifest_version": 2,
	"permissions": [
		"storage",
		"https://github.com/*",
		"https://ghloc.bytes.pw/*"
	],
	"content_scripts": [
		{
			"matches": ["https://github.com/*"],
			"js": ["content-script.js"]
		}
	]
}
